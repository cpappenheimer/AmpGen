    "version": "0.2.0",
    "configurations": [
        {
            "name": "AmpGenDebugger(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/data/home/pappenheimer/develop_AmpGen/build_debug/bin/Debugger",
            "args": [
                "/data/home/pappenheimer/develop_AmpGen/build_debug/bin/zenodo_version_D02Kpipipi.opt",
                "--EventType",
                "D0 K- pi+ pi+ pi-",
                "--GenerateTimeDependent",
                "--CoherentSum::Debug"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
       
    ]
}